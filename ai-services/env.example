# RespiCare AI Services Environment Configuration
# IMPORTANT: Copy this file to .env and fill in your actual values

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=RespiCare AI Services
VERSION=1.0.0

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:8080

# Database Configuration
DATABASE_URL=mongodb://username:password@host:port/database?authSource=admin
REDIS_URL=redis://host:port

# AI/ML Configuration
OPENAI_API_KEY=your_openai_api_key_here
MODEL_PATH=/app/models
CACHE_PATH=/app/cache

# Model Names
MEDICAL_MODEL_NAME=en_core_sci_sm
SYMPTOM_MODEL_NAME=symptom-classifier-v1
HISTORY_MODEL_NAME=medical-history-processor-v1

# Processing Configuration
MAX_TEXT_LENGTH=10000
BATCH_SIZE=32
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO